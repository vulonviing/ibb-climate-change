    "import os\n",
    "import json\n",
    "\n",
    "# Verilerin bulunduğu klasör yolunu belirtin\n",
    "folder_path = 'weather datas turkey/'\n",
    "\n",
    "# Klasördeki dosya adlarını döngüyle geziyoruz\n",
    "for filename in os.listdir(folder_path):\n",
    "    if filename.startswith('weather_data_'):\n",
    "        # Dosya adının parçalarını ayırıyoruz\n",
    "        parts = filename.split('_')\n",
    "        \n",
    "        if len(parts) > 2:\n",
    "            city_name = '_'.join(parts[2:])  # Şehir adını alıyoruz\n",
    "            new_filename = f'weather_data_Türkiye_{city_name}'\n",
    "            \n",
    "            # Eski dosya yolunu ve yeni dosya yolunu oluşturuyoruz\n",
    "            old_file_path = os.path.join(folder_path, filename)\n",
    "            new_file_path = os.path.join(folder_path, new_filename)\n",
    "            \n",
    "            # Dosya adını güncelliyoruz\n",
    "            os.rename(old_file_path, new_file_path)\n",
    "\n",
    "            # Dosyanın içeriğini okuyup yeniden yazabilirsiniz, burada örnek olarak print ediyoruz\n",
    "            with open(new_file_path, 'r') as json_file:\n",
    "                data = json.load(json_file)\n",
    "                print(data)\n",
    "                \n",
    "            print(f'Dosya adı güncellendi: {filename} -> {new_filename}')"
   ]
