    "# API'dan veriyi çekme\n",
    "url = \"http://api.worldweatheronline.com/premium/v1/past-weather.ashx?key=59517f469a3e43ad8f0125422232608&q=Istanbul&format=json&date=2009-01-01&enddate=2023-08-25\"\n",
    "response = requests.get(url)\n",
    "data = response.json()\n",
    "\n",
    "# Veriyi JSON dosyasına kaydetme\n",
    "with open(\"weather datas/weather_data_Istanbul.json\", \"w\") as json_file:\n",
    "    json.dump(data, json_file, indent=4)  # indent=4 ile daha okunaklı bir JSON dosyası oluşturulur\n",
    "\n",
    "# JSON dosyasını oku\n",
    "with open('weather datas/weather_data_Istanbul.json', 'r') as json_file:\n",
    "    data = json.load(json_file)\n",
    "\n",
    "# DataFrame oluştur\n",
    "weather_data = data['data']['weather']\n",
    "dftemp = pd.json_normalize(weather_data).drop(columns=[\"hourly\",\"astronomy\"])  # JSON içindeki verileri DataFrame'e dönüştür\n",
    "dfastronomy = pd.json_normalize(weather_data, record_path='astronomy', meta=['date'])\n",
    "dtemp_hourly = pd.json_normalize(weather_data, record_path='hourly', meta=['date'])\n",
    "dftemp[\"City\"] = \"Istanbul\"\n",
    "dfastronomy[\"City\"] = \"Istanbul\"\n",
    "dtemp_hourly[\"City\"] = \"Istanbul\""
   ]
  },
